<script>
  var slider = new KeenSlider("#my-keen-slider", {
    loop: {
      min: -5,
      max: 5,
    },
  });

  var domestic = document.getElementById("domestic");
  var destination_container = document.getElementById("destination_container");

  let hideTimeout;
  domestic.addEventListener("mouseenter", () => {
    clearTimeout(hideTimeout);
    destination_container.classList.remove("hidden");
  });
  domestic.addEventListener("mouseleave", () => {
    hideTimeout = setTimeout(() => {
      destination_container.classList.add("hidden");
    }, 2000);
  });
  var links = document.getElementsByClassName("link");
  for (var link in links) {
    link.addEventListener("mouseenter", () => {
      destination_container.classList.remove("hidden");
    });
    link.addEventListener("mouseleave", () => {
      destination_container.classList.add("hidden");
    });
  }

  // Initialization for ES Users
import {
  Modal,
  Ripple,
  initTE,
} from "tw-elements";

initTE({ Modal, Ripple });
</script>
